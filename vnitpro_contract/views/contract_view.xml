                                    </field>
                                    <div colspan="4" class="center">
                                        <button string="Acceptance - Payment Confirm" type="object"
                                                name="acceptance_payment"
                                                confirm="Are you sure to acceptance payment this contract?"
                                                class="oe_highlight oe_right"
                                                attrs="{'invisible': [('status','!=','asset_increase')]}"/>
                                    </div>
                                </group>
                            </page>
                            <page colspan="4" string="Liquidation" attrs="{'invisible':[('id', '&lt;', 1)]}">
                                <group col="4">
                                    <div colspan="4" class="div_center">Liquidation</div>
                                    <field name="information" colspan="4" readonly="1"/>
                                    <field name="liquidation_number" colspan="2" readonly="1"/>
                                    <field name="liquidation_date" colspan="2" readonly="1"/>
                                    <field name="total_contract_price"/>
                                    <field name="currency_liquidation" style="width:50%%"/>
                                    <field name="total_paid_contract_price" colspan="4" style="width:50%%"/>
                                    <field name="remain_contract_price" colspan="4" style="width:50%%"/>
                                    <div colspan="4" class="center">
                                        <button string="Liquidation Confirm" type="object"
                                                name="liquidated_finalized" class="oe_highlight"
                                                confirm="Are you sure to liquidation this contract?"
                                                attrs="{'invisible': [('status','!=','acceptance_payment')]}"/>&#x2063;&#x2063;&#x2063;
                                        <button string="Edit" type="object" name="edit_liquidation"
                                                class="oe_highlight"
                                                attrs="{'invisible': [('status','!=','acceptance_payment')]}"/>&#x2063;&#x2063;&#x2063;
                                        <button string="Delete" type="object" name="delete_liquidation"
                                                attrs="{'invisible': [('status','!=','acceptance_payment')]}"
                                                class="oe_highlight" confirm="Are you sure to delete?"/>
                                    </div>
                                </group>
                                <group colspan="4" name="des" attrs="{'invisible':[('id', '&lt;', 1)]}">
                                    <div colspan="4" class="div_attachfile">Attachment Files</div>
                                    <field name="file_attachment_liquidation_ids" nolabel="1">
                                        <tree editable="bottom">
                                            <field name="file" filename="file_name"/>
                                            <field name="file_name" force_save="1"/>
                                            <field name="user_id" force_save="1"/>
                                            <field name="description"/>
                                            <field name="date"/>
                                            <button name="download_file" type="object"
                                                    class="button_blank fa fa-download"/>
                                        </tree>
                                    </field>
                                </group>
                            </page>
                            <page colspan="4" string="Contract Permissions" attrs="{'invisible':[('id', '&lt;', 1)]}">
                                <group>
                                    <div colspan="4" class="div_center">Contract Permissions</div>
                                    <field name="permission_detail_ids" nolabel="1">
                                        <tree editable="top" limit="30">
                                            <field name="employee_id" options="{'no_open':True}"/>
                                            <field name="user_id" invisible="1"/>
                                            <field name="perm_read"/>
                                            <field name="perm_write"/>
                                            <field name="perm_unlink"/>
                                        </tree>
                                        <form>
                                            <sheet>
                                                <group col="4">
                                                    <field name="employee_id" widget="selection"/>
                                                    <field name="perm_read"/>
                                                    <field name="perm_write"/>
                                                    <field name="perm_unlink"/>
                                                    <field name="user_id" invisible="1"/>
                                                </group>
                                            </sheet>
                                        </form>
                                    </field>
                                </group>
                            </page>
                        </notebook>
                        <field name="contract_kind" invisible="1"/>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
            </field>
        </record>
        <record id="view_vnitpro_contract_tree" model="ir.ui.view">
            <field name="name">vnitpro.contract.tree</field>
            <field name="model">vnitpro.contract</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <tree string="Contract" name="contract">
                    <field name="code"/>
                    <field name="name"/>
                    <field name="bidder_a"/>
                    <field name="bidder_b"/>
                    <field name="contract_creator"/>
                    <field name="create_date"/>
                    <field name="status"/>
                    <field name="state" class="red_color" string=" "
                           attrs="{'invisible':[('status', '=', 'pending')]}"/>
                </tree>
            </field>
        </record>
        <record model="ir.actions.act_window" id="act_open_vnitpro_input_contract_view">
            <field name="name">Input Contract</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">vnitpro.contract</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_vnitpro_contract_search"/>
            <field name="domain">[]</field>
            <field name="context">{
                'default_contract_kind':'input',
                'search_default_input_contract':1,
                }
            </field>
        </record>
        <record model="ir.actions.act_window.view" id="act_open_vnitpro_input_contract_tree_view">
            <field name="act_window_id" ref="act_open_vnitpro_input_contract_view"/>
            <field name="sequence" eval="8"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_vnitpro_contract_tree"/>
        </record>
        <record model="ir.actions.act_window.view" id="act_open_vnitpro_input_contract_form_view">
            <field name="act_window_id" ref="act_open_vnitpro_input_contract_view"/>
            <field name="sequence" eval="8"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_vnitpro_contract_form"/>
        </record>
        <record model="ir.actions.act_window" id="act_open_vnitpro_output_contract_view">
            <field name="name">Output Contract</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">vnitpro.contract</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_vnitpro_contract_search"/>
            <field name="domain">[]</field>
            <field name="context">{
                'default_contract_kind':'output',
                'search_default_output_contract':1,
                }
            </field>
        </record>
        <record model="ir.actions.act_window.view" id="act_open_vnitpro_output_contract_tree_view">
            <field name="act_window_id" ref="act_open_vnitpro_output_contract_view"/>
            <field name="sequence" eval="8"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_vnitpro_contract_tree"/>
        </record>
        <record model="ir.actions.act_window.view" id="act_open_vnitpro_output_contract_form_view">
            <field name="act_window_id" ref="act_open_vnitpro_output_contract_view"/>
            <field name="sequence" eval="8"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_vnitpro_contract_form"/>
        </record>
    </data>
</odoo>