<?xml version="1.0" encoding="utf-8"?>
<templates id='template' xmlspace='preserve'>
    <!--Extends UserMenu -->
    <t t-extend="UserMenu.Actions">
        <t t-jquery="a[data-menu='account'], a[data-menu='help'], a[data-menu='about']">
            this.parent().remove();
        </t>
        <t t-jquery="a[data-menu='support']">
            this.parent().remove();
        </t>
        <t t-jquery="a[data-menu='documentation']">
            this.parent().remove();
        </t>
    </t>
</templates>