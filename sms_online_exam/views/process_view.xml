<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_sms_online_exam_process_tree" model="ir.ui.view">
            <field name="name">sms.online.exam.process.tree</field>
            <field name="model">sms.online.exam.process</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <tree>
                    <field name="code"/>
                    <field name="name"/>
                    <field name="activate"/>
                </tree>
            </field>
        </record>
        <record id="view_sms_online_exam_process_form" model="ir.ui.view">
            <field name="name">sms.online.exam.process.form</field>
            <field name="model">sms.online.exam.process</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form>
                    <header>
                        <field name="activate" widget="statusbar" clickable="True"
                            options="{'fold_field': 'fold'}"/>
                    </header>
                    <sheet>
                        <group col="4">
                            <field name="code"/>
                            <field name="name"/>
                        </group>
                        <group>
                            <field name="description"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_sms_online_exam_process_search" model="ir.ui.view">
            <field name="name">sms.online.exam.process.search</field>
            <field name="model">sms.online.exam.process</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <search>
                    <field name="name"/>
                    <field name="code"/>
                    <field name="activate"/>
                </search>
            </field>
        </record>
        <record model="ir.actions.act_window" id="act_open_sms_online_exam_process_view">
            <field name="name">Process</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">sms.online.exam.process</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_sms_online_exam_process_search"/>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>
        <record model="ir.actions.act_window.view" id="act_open_sms_online_exam_process_view_form">
            <field name="act_window_id" ref="act_open_sms_online_exam_process_view"/>
            <field name="sequence" eval="20"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_sms_online_exam_process_form"/>
        </record>
        <record model="ir.actions.act_window.view" id="act_open_sms_online_exam_process_view_tree">
            <field name="act_window_id" ref="act_open_sms_online_exam_process_view"/>
            <field name="sequence" eval="10"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_sms_online_exam_process_tree"/>
        </record>
    </data>
</odoo>
